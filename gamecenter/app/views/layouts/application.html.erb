<!DOCTYPE html>
<html>
  <head>
    <title>Gamecenter</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
<header>
    <% if user_signed_in? %>
    <%= link_to "拠点", user_path(current_user.id) %>
    <%= link_to "メンバー", users_path %>
    <%= link_to "逃げる", destroy_user_session_path, method: :delete %>
  <% end %>
</header>
    <%= yield %>

    <footer>
      <p>this is a footer.</p>
    </footer>
  </body>
  


  
</html>
